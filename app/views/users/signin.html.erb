<h1>Sign In</h1>
<%= form_with(model: @users) do |form| %>
  
    <div class="field">
      <%= form.label :email %>
      <%= form.text_field :email %>
    </div>
  
    <div class="field">
      <%= form.label :password %>
      <%= form.text_field :password %>
    </div>

    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
  

<%= link_to 'Register', new_user_path %>
